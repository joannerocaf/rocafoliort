  "scripts": {
    "build": "sass sass/styles.scss build/style.css && pug index.pug --out build && pug pages --out build/pages",
    "pug": "yarn pugc --watch",
    "pugc": "pug index.pug pages",
    "sass": "yarn sassc --watch",
