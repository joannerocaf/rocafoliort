{
	"name": "rocafoliort_site",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"start": "node ./public/server.js",
		"serve": "npm run build && run-p 'build:* -- --watch' watch:*",
		"build": "run-p build:*",
		"prepack": "rm -rf package *.tgz && npm run build",
		"postpack": "tar -xzf *.tgz && rm *.tgz",
		"build:sass": "sass public/style.scss public/style.css",
		"build:tsc": "tsc -b",
		"watch:server": "node --inspect --watch ./public/server.js"
	},
	"dependencies": {
		"bootstrap": "5.3.1",
		"compression": "1.7.4",
		"express": "4.18.2",
		"font-awesome": "4.7.0",
		"pug": "3.0.2",
		"jquery": "3.7.1",
		"popper.js": "1.16.1"
	},
	"devDependencies": {
		"@types/compression": "1.7.3",
		"@types/express": "4.17.17",
		"@types/pug": "2.0.6",
		"npm-run-all": "4.1.5",
		"sass": "1.66.1",
		"typescript": "5.2.2"
	}
}
