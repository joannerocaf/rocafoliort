{
	"name": "rocafoliort_site",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"license": "MIT",
	"scripts": {
		"start": "node --experimental-json-modules ./public/server.js",
		"build": "npm run sass && npm run tsc",
		"lint": "tslint -p .",
		"sass": "sass public/style.scss public/style.css",
		"tsc": "tsc",
		"watch": "onchange -i -k 'public/*.js' -- npm start",
		"serve": "run-p 'sass -- --watch' 'tsc -- --watch' watch"
	},
	"dependencies": {
		"bootstrap": "5.1.3",
		"compression": "1.7.4",
		"express": "4.17.2",
		"font-awesome": "4.7.0",
		"pug": "3.0.2",
		"jquery": "3.6.0",
		"popper.js": "1.16.1"
	},
	"devDependencies": {
		"@ledge/configs": "23.3.23322",
		"@types/compression": "1.7.2",
		"@types/express": "4.17.13",
		"@types/pug": "2.0.5",
		"npm-run-all": "4.1.5",
		"onchange": "7.1.0",
		"sass": "1.45.1",
		"tslint": "6.1.3",
		"typescript": "4.5.4"
	}
}
