    "start": "nodemon ./node_modules/local-web-server/bin/cli.js"
	},
  "dependencies": {
    "bootstrap": "^4.3.1",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0"
  },
  "devDependencies": {
		"local-web-server": "^3.0.7",
		"nodemon": "^1.19.3",
    "pug": "^2.0.4",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.23.0"
  }
}
