{
	"compilerOptions": {
		"lib": ["es2021"],
		"target": "es2021",
		"module": "NodeNext",
		"esModuleInterop": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"strict": true
	}
}
